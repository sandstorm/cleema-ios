--maxwidth 120
--wraparguments before-first
--wrapcollections before-first
--exclude "Package.swift", ".swiftpm", "Generated"
--indent 4
--header "\n Created by Kumpels and Friends on {created}.\n Copyright Â© {year} Kumpels and Friends. All rights reserved.\n"
--binarygrouping none
--commas inline
--decimalgrouping 3
--hexgrouping none
--hexliteralcase lowercase
--ifdef no-indent
--octalgrouping none
--semicolons never
--disable unusedArguments
--disable preferKeyPath
